algoritmo "Vetor"
var
   v : vetor[1..4] de Inteiro
   i : Inteiro
inicio
      // Leia os valores para o vetor
      Para i := 1 ate 4 faca
           Escreva("v[",i,"] = ")
           Leia(v[i])
      FimPara
      
      // Mostre os valores para user
      Para i := 1 ate 4 faca
           EscrevaL("v[",i,"] = ", v[i])
      FimPara
      
fimalgoritmo
