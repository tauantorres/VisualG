algoritmo "Aleatorio"
var
      v : Inteiro
inicio
      aleatorio on
      Escreva("Digite um valor: ")
      Leia(v)
      EscrevaL("Valor digitado foi: ", v)
      aleatorio off
      
      Escreva("Digite outro valor: ")
      Leia(v)
      EscrevaL("Valor digitado foi: ", v)

fimalgoritmo
